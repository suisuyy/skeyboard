<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"

    >

    <Row>
        <Key android:codes="49" android:keyLabel="1" android:keyEdgeFlags="left" android:popupCharacters="!"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="50" android:keyLabel="2" android:popupCharacters="\@"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="51" android:keyLabel="3" android:popupCharacters="#"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="52" android:keyLabel="4" android:popupCharacters="$"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="53" android:keyLabel="5" android:popupCharacters="%"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="54" android:keyLabel="6" android:popupCharacters="^"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="55" android:keyLabel="7" android:popupCharacters="&amp;"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="56" android:keyLabel="8" android:popupCharacters="*"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="57" android:keyLabel="9" android:popupCharacters="("  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="48" android:keyLabel="0" android:keyEdgeFlags="right" android:popupCharacters=")"  android:popupKeyboard="@xml/popup_keyboard_template"/>
    </Row>
    <Row>
        <Key android:codes="113" android:keyLabel="q" android:keyEdgeFlags="left" android:popupCharacters="`"  android:popupKeyboard="@xml/popup_keyboard_template" />
        <Key android:codes="119" android:keyLabel="w" android:popupCharacters="~"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="101" android:keyLabel="e" android:popupCharacters="\\"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="114" android:keyLabel="r" android:popupCharacters="|"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="116" android:keyLabel="t" android:popupCharacters="_"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="121" android:keyLabel="y" android:popupCharacters="-"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="117" android:keyLabel="u" android:popupCharacters="+"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="105" android:keyLabel="i" android:popupCharacters="="  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="111" android:keyLabel="o" android:popupCharacters="{"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="112" android:keyLabel="p" android:keyEdgeFlags="right" android:popupCharacters="}"  android:popupKeyboard="@xml/popup_keyboard_template"/>
    </Row>
    
    <Row>
        <Key android:codes="9" android:keyLabel="Tab"
            android:keyWidth="10%p" android:keyEdgeFlags="left"/>
        <Key android:codes="97" android:keyLabel="a" />
        <Key android:codes="115" android:keyLabel="s"/>
        <Key android:codes="100" android:keyLabel="d" android:popupCharacters=":"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="102" android:keyLabel="f" android:popupCharacters="&#39;"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="103" android:keyLabel="g" android:popupCharacters="&#34;"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="104" android:keyLabel="h" android:popupCharacters="&lt;"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="106" android:keyLabel="j" android:popupCharacters="&gt;"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="107" android:keyLabel="k" android:popupCharacters="["  />
        <Key android:codes="108" android:keyLabel="l" android:popupCharacters="]"  android:popupKeyboard="@xml/popup_keyboard_template" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-53" android:keyLabel="Shift"
            android:keyWidth="15%p" android:isModifier="true"
            android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="122" android:keyLabel="z" android:popupKeyboard="@xml/qwerty" />
        <Key android:codes="120" android:keyLabel="x" android:popupCharacters=":"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="99" android:keyLabel="c" android:popupCharacters=","  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="118" android:keyLabel="v" android:popupCharacters=";"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="98" android:keyLabel="b" android:popupCharacters="."  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="110" android:keyLabel="n" android:popupCharacters="/"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="109" android:keyLabel="m" android:popupCharacters="\?"  android:popupKeyboard="@xml/popup_keyboard_template"/>
        <Key android:codes="8" android:keyLabel="&lt;&lt;&lt;"
            android:keyWidth="15%p" android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="27" android:keyLabel="Esc"
                android:keyWidth="10%p" android:keyEdgeFlags="left"/>
        <!--<Key android:codes="-2" android:keyLabel="123" android:keyWidth="10%p"/>-->
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.suisuy.suinput.LatinKeyboardView.
        -->
        <!--<Key android:codes="-101" android:keyIcon="@drawable/sym_keyboard_language_switch"
            android:keyWidth="10%p"/>-->
        <Key android:codes="-52" android:keyLabel="Ctrl"
                android:keyWidth="10%p"/>
        <Key android:codes="-51" android:keyLabel="Alt"
            android:keyWidth="10%p"/>
            
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyLabel="space"
                android:keyWidth="40%p" android:isRepeatable="true"/>
       
        <Key android:codes="10" android:keyLabel="Enter"
                android:keyWidth="30%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>


    
